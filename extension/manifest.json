{
    "manifest_version": 3,
    "name": "Phishing URL Detector",
    "description": "An extension to predict phishing urls.",
    "version": "1.0",  
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js": ["index.js"]
    }],
    "host_permissions":["<all_urls>"],
    "permissions":["storage","tabs"]
}